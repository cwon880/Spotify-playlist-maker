{
    "manifest_version":2,
    "name": "Spotify playlist maker extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":[
                "https://*.youtube.com/*"
            ],
            "js": ["jquery-3.4.1.min.js","content.js"],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "playlistmaker.png",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "identity",
        "tabs",
        "https://*.spotify.com/*"
    ]
}