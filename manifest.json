{
    "manifest_version":2,
    "name": "Spotify playlist maker extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":[
                "https://*.youtube.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "playlistmaker.png",
        "default_popup": "popup.html"
    }
}